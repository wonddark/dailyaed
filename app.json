{
  "expo": {
    "name": "DailyAED",
    "slug": "dailyaed",
    "version": "0.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "dailyaed",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.wonddark.dailyaed",
      "infoPlist": {
        "ExpoLocalization_supportsRTL": true,
        "CFBundleLocalizations": ["en", "es", "ar", "en", "es", "ar"],
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.wonddark.dailyaed"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      [
        "expo-localization",
        {
          "supportedLocales": {
            "ios": ["en", "es", "ar"],
            "android": ["en", "es", "ar"]
          }
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Inter.ttf",
            "./assets/fonts/Poppins-Light.ttf"
          ]
        }
      ]
    ],
    "extra": {
      "supportsRTL": true,
      "router": {},
      "eas": {
        "projectId": "8b884992-74ba-4cac-83df-1f88e274b403"
      }
    },
    "owner": "wonddark"
  }
}
